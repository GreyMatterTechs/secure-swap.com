{
	"rules": {
		"no-console": process.env.NODE_ENV === undefined ? 0 : 2,
    	"no-debugger": process.env.NODE_ENV === undefined ? 0 : 2,
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
		],
		"no-multiple-empty-lines": [
			"error", 
			{ "max": 2, "maxEOF": 1 }
		]
	},
	"extends": "loopback",
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"amd": true,
		"jquery": true
  	}
}