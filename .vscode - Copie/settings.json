{
	"git.ignoreLimitWarning": true,

	// An array of globs used to include and exclude workspace paths.
	"gulptasks.pattern": "**/gulpfile*.js",
	"gulptasks.filters": [
		"!node_modules/**",
		"!bower_components/**",
		"!assets/**",
		"!gulp-tasks/**",
		"!resources/**",
		"!src/**",
		"!theme-assets/**"
	],
	"gulptasks.notifications.executed": true,
	"gulptasks.notifications.terminated": true,
	"gulptasks.notifications.restarted": true

}