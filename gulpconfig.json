{
  "html": "html",
        "assets": "client/assets",
        "theme_assets": "client/theme-assets",
        "assets_path": "../../../assets",
        "theme_assets_path": "../../../theme-assets",
  "assets-dest": {
    "css": "client/assets/css",
    "js": "client/assets/js"
  },
  "theme-assets-dest": {
    "css": "client/theme-assets/css",
    "js": "client/theme-assets/js"
  },
  "assets-source": {
    "path": "client/assets-src",
    "js": "client/assets-src/js",
    "sass": "client/assets-src/scss"
  },
  "theme-assets-source": {
    "path": "client/theme-assets-src",
    "js":   "client/theme-assets-src/js",
    "sass": "client/theme-assets-src/scss"
  },
  "autoprefixerBrowsers": [
    "Android 2.3",
    "Android >= 4",
    "Chrome >= 25",
    "Firefox >= 15",
    "Explorer >= 9",
    "iOS >= 6",
    "Opera >= 12",
    "Safari >= 6"
  ]
}