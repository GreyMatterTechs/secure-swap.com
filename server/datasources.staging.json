	"emailDS": {
		"name": "emailDS",
		"connector": "mail",
		"transports": [
			{
				"type": "smtp",
				"secure": true,
				"port": 465,
				"host": "${SMTP_HOST}",
				"auth": {
					"user": "${SMTP_USER}",
					"pass": "${SMTP_PASS}"
				}
			}
		]
	}
}